{
  "name": "utmn-security-monorepo",
  "version": "1.0.0",
  "description": "Система безопасности инфраструктуры ТюмГУ - Monorepo",
  "private": true,
  "scripts": {
    "install:frontend": "cd frontend && npm install",
    "install:backend": "cd backend && npm install",
    "install:all": "npm run install:frontend && npm run install:backend",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && npm start",
    "build:frontend": "cd frontend && npm run build",
    "build:all": "npm run build:frontend",
    "clean:frontend": "cd frontend && rm -rf node_modules dist",
    "clean:backend": "cd backend && rm -rf node_modules",
    "clean:all": "npm run clean:frontend && npm run clean:backend"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tyumgu/security-system"
  },
  "keywords": [
    "security",
    "tyumgu",
    "access-control",
    "monorepo"
  ],
  "author": "TyumGU",
  "license": "MIT"
}
